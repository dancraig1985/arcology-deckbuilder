; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "CardLibrary",
"language": "GDScript",
"path": "res://core/classes/card_library.gd"
}, {
"base": "Reference",
"class": "StackMachineState",
"language": "GDScript",
"path": "res://core/classes/stack_machine/stack_machine_state.gd"
} ]
_global_script_class_icons={
"CardLibrary": "",
"StackMachineState": ""
}

[application]

config/name="arcology-deckbuilder"
run/main_scene="res://core/scenes/main.tscn"
config/icon="res://icon.png"

[autoload]

Constants="*res://core/autoloads/constants.gd"
Audio="*res://core/autoloads/audio.tscn"
RNG="*res://core/autoloads/rng.gd"

[debug]

gdscript/warnings/unused_variable=false
gdscript/warnings/unused_argument=false
gdscript/warnings/return_value_discarded=false

[display]

window/size/width=1600
window/size/height=900

[rendering]

vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0, 0, 0, 1 )
environment/default_environment="res://default_env.tres"
